GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/Cordova" "${PODS_ROOT}/Headers/Public/LigerMobile" "${PODS_ROOT}/Headers/Public/UIColor-HTMLColors"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/Cordova" -isystem "${PODS_ROOT}/Headers/Public/LigerMobile" -isystem "${PODS_ROOT}/Headers/Public/UIColor-HTMLColors"
OTHER_LDFLAGS = -ObjC -l"Pods-Template-Cordova" -l"Pods-Template-LigerMobile" -l"Pods-Template-UIColor-HTMLColors" -framework "AVFoundation" -framework "AssetsLibrary" -framework "CoreLocation" -framework "MobileCoreServices"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_ROOT = ${SRCROOT}/Pods